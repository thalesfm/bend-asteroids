@List/Cons = (a (b ((@List/Cons/tag (a (b c))) c)))

@List/Cons/tag = 1

@List/Nil = ((@List/Nil/tag a) a)

@List/Nil/tag = 0

@app/App = (a (b (c ((@app/App/tag (a (b (c d)))) d))))

@app/App/tag = 0

@draw = (* d)
  & @List/Cons ~ (1 (c d))
  & @List/Cons ~ (2 (b c))
  & @List/Cons ~ (3 (a b))
  & @List/Cons ~ (4 (@List/Nil a))

@init = 10

@main = a
  & @app/App ~ (@init (@tick (@draw a)))

@tick = ($([+0x0000001] a) a)


